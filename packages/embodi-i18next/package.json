{
  "name": "embodi-i18next",
  "version": "0.1.0",
  "description": "adds i18n support to embodi",
  "type": "module",
  "main": "dist/exports/lib.js",
  "types": "dist/exports/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/exports/lib.d.ts",
      "import": "./dist/exports/lib.js"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build && publint",
    "dev": "tsc --watch --project tsconfig.json",
    "build": "tsc --project tsconfig.json"
  },
  "keywords": [
    "embodi",
    "i18n",
    "i18next"
  ],
  "author": "Wolfgang Rathgeb",
  "license": "Apache-2.0",
  "peerDependencies": {
    "typescript": "^5.0.0",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "i18next": "^23.16.4"
  }
}
