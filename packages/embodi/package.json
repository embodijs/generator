{
	"name": "embodi",
	"version": "0.9.0",
	"license": "Apache-2.0",
	"module": "dist/exports/lib.js",
	"types": "dist/exports/lib.d.ts",
	"bin": {
		"embodi": "bin/embodi.js"
	},
	"files": [
		"dist",
		"client.d.ts"
	],
	"scripts": {
		"dev": "svelte-package -i src --watch",
		"test:unit": "vitest",
		"build": "svelte-package -i src",
		"prepublishOnly": "npm run build && publint"
	},
	"keywords": [
		"@embodi",
		"svelte",
		"vite",
		"generator",
		"typescript",
		"bundler"
	],
	"exports": {
		".": {
			"types": "./dist/exports/lib.d.ts",
			"import": "./dist/exports/lib.js"
		},
		"./client": {
			"types": "./client.d.ts"
		}
	},
	"type": "module",
	"devDependencies": {
		"@loom-io/in-memory-adapter": "0.12.10",
		"@sveltejs/package": "^2.3.6",
		"@types/markdown-it": "^14.1.2",
		"@types/minimist": "^1.2.5",
		"rollup": "^4.24.0"
	},
	"peerDependencies": {
		"svelte": "^5.1.0",
		"typescript": "^5.0.0",
		"vite": "^5.2.11"
	},
	"dependencies": {
		"@loom-io/converter": "0.12.10",
		"@loom-io/core": "^0.12.10",
		"@loom-io/front-matter-converter": "^0.12.10",
		"@loom-io/json-converter": "0.12.10",
		"@loom-io/node-filesystem-adapter": "^0.12.10",
		"@loom-io/yaml-converter": "0.12.10",
		"@modyfi/vite-plugin-yaml": "^1.1.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"front-matter": "^4.0.2",
		"markdown-it": "^14.1.0",
		"minimist": "^1.2.8",
		"pipe-and-combine": "^0.5.7",
		"vitest": "^2.1.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/embodijs/generator/tree/main/packages/embodi"
	}
}
